#include<bits/stdc++.h>
using namespace std;

// set is the subset
// sos is sum of subset
// tar is target
void printTargetSumSubsets(vector<int> arr, int idx, string set, int sos, int tar) {
    if(tar == sos) {
        cout << set << "." << endl;
        return;
    }
    if (idx == arr.size()-1) {
        if(tar == sos) {
            cout << set << "." << endl;
            return;
        }
    } 
    if (idx >= arr.size()) {
        return;
    }
   
    //cout << "idx:" << idx << endl;
//    cout << "arr[idx]" << arr[idx] << endl;
//    cout << "set+to_string(arr[idx])+" << set+to_string(arr[idx])+"," << endl;
//    cout << "sos:" << sos+arr[idx] << endl;
    printTargetSumSubsets(arr, idx+1, set+to_string(arr[idx])+","+" ", sos+arr[idx], tar);
    
    printTargetSumSubsets(arr, idx+1, set, sos, tar);
    
}


int main() {
    
    int n       =   0;
    int target  =   0;
    
    cin >> n;
    
    vector<int> arr(n,0);
   // cout << n << endl;
    for(int i=0; i<n; i++) {
        cin >> arr[i];
       // cout << to_string(arr[i]) << endl;
    }
    cin >> target;
    //cout << "target:" << target << endl;
    //cout << arr.size() << endl;
    printTargetSumSubsets(arr, 0, "", 0, target);

}
